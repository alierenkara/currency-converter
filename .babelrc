{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
    ],
    "plugins": [
        "@babel/plugin-transform-runtime",
        [
            "module-resolver",
            {
              "root": ["./src"],
              "alias": {
                "components": "./src/components",
                "constants": "./src/constants",
                "pages": "./src/pages",
                "services": "./src/services",
                "utils": "./src/utils"
              }
            }
          ]
    ]
}